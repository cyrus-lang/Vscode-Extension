{
  "name": "cyrus-language",
  "displayName": "Cyrus Programming Language",
  "description": "The Cyrus Programming Language",
  "version": "0.0.35",
  "engines": {
    "vscode": "^1.96.0"
  },
  "publisher": "cyrus-lang-tools",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyrus-lang/Vscode-Extension"
  },
  "scripts": {
    "test": "vscode-test",
    "publish": "vsce publish",
    "package": "vsce package"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "cyrus",
        "path": "./snippets/variables.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/functions.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/conditionals.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/loops.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/structs.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/enums.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/unions.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/tuples.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/modules.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/imports.code-snippets"
      },
      {
        "language": "cyrus",
        "path": "./snippets/misc.code-snippets"
      }
    ],
    "languages": [
      {
        "id": "cyrus",
        "aliases": [
          "Cyrus"
        ],
        "extensions": [
          ".cyrus"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cyrus",
        "scopeName": "source.cyrus",
        "path": "./syntaxes/cyrus.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "vsce": "^2.15.0"
  }
}
